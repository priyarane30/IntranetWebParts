{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "poll-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/poll/PollWebPart.js",
          "manifest": "./src/webparts/poll/PollWebPart.manifest.json"
        }
      ]
    },
    "main-2-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/main2/Main2WebPart.js",
          "manifest": "./src/webparts/main2/Main2WebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "PollWebPartStrings": "lib/webparts/poll/loc/{locale}.js",
    "Main2WebPartStrings": "lib/webparts/main2/loc/{locale}.js"
  }
}
